# Echocraft Landing Page Environment Variables
# Copy this file to .env and update with your values

# Google Analytics Tracking ID
VITE_GA_TRACKING_ID=G-XXXXXXXXXX

# API Base URL (for future integrations)
VITE_API_BASE_URL=https://api.echocraft.io

# Contact Email
VITE_CONTACT_EMAIL=hello@echocraft.io

# Enable Analytics (true/false)
VITE_ENABLE_ANALYTICS=true

# Company Information
VITE_COMPANY_NAME=Echocraft Technologies
VITE_COMPANY_PHONE=+1-888-555-ECHO
VITE_COMPANY_ADDRESS=123 Innovation Blvd, San Francisco, CA 94107

# Social Media Links
VITE_TWITTER_URL=https://twitter.com/echocraft
VITE_LINKEDIN_URL=https://linkedin.com/company/echocraft
VITE_GITHUB_URL=https://github.com/echocraft

# Feature Flags
VITE_ENABLE_CHATBOT=false
VITE_ENABLE_CONTACT_FORM=true
VITE_ENABLE_NEWSLETTER=true
